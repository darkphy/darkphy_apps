    "test": "cross-env NODE_ENV=test jest src/{,**/}*.spec.js",
    "clean": "rimraf build",
    "copy:shared": "cp -r ../shared src/shared",
    "clean:shared": "rimraf src/shared"
  },
  "devDependencies": {
